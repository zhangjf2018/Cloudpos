<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="withDrawCashDetailModel"
            type="com.ymt.cloudpos.model.WithDrawCashDetailModel" />
    </data>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:paddingBottom="10dp"
    >
    <TextView
        android:id="@+id/tv_transType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="21sp"
        android:textColor="@color/black"
        android:text="@{withDrawCashDetailModel.transType}"/>

    <TextView
        android:id="@+id/tv_withDrawCashAmt"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_centerVertical="true"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:text="@{withDrawCashDetailModel.withDrawCashAmt}"/>

    <TextView
        android:id="@+id/tv_withDrawCashTime"
        android:paddingTop="2dp"
        android:layout_below="@+id/tv_transType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:text="@{withDrawCashDetailModel.withDrawCashTime}"/>
</RelativeLayout>
</layout>