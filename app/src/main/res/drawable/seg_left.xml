<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_selected="true">
        <shape  >
            <stroke android:color="#FFA633" android:width="1dp"/>
            <solid android:color="#FFA633"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item>
        <shape >
            <stroke android:color="@color/alphaAll" android:width="0dp"/>
            <solid android:color="@color/alphaAll"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
</selector>